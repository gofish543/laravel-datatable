{
	"name": "dykhuizen/laravel-datatable",
	"description": "A Laravel package that contains queries for constructing a datatable",
	"keywords": [
		"sorting",
		"filtering",
		"selecting",
		"paginating",
		"searching",
		"laravel"
	],
	"homepage": "https://github.com/gofish543/laravel-datatable",
	"license": "MIT",
	"authors": [
		{
			"name": "Nicholas Dykhuizen",
			"email": "nicholas@dykhuizen.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.3 | ^8.0",
		"illuminate/database": "^7.0 | ^8.0",
		"illuminate/support": "^7.0 | ^8.0" 
	},
	"require-dev": {
		"orchestra/testbench": "^6.0",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"Dykhuizen\\Datatable\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Dykhuizen\\Datatable\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Dykhuizen\\Datatable\\DatatableServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
